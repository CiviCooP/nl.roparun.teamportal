{
  "name": "portal_deactiveer_teamlid",
  "title": "Portal: deactiveer teamlid",
  "is_active": "1",
  "description": "Deze form processer annuleert een team lid. Dat betekent dat het deelnemers record op geannuleerd wordt gezet.",
  "output_handler": "FormatOutput",
  "output_handler_configuration": {
    "fields": [
      {
        "field": "input.contact_id",
        "output_name": "contact_id"
      },
      {
        "field": "input.team_id",
        "output_name": "team_id"
      }
    ]
  },
  "enable_default_data": "1",
  "default_data_output_configuration": {
    "donatie_status_teamlid": "action.donatie_status.value",
    "teamlid_op_website": "action.teamlid_op_website.value"
  },
  "permission": "access Team Portal API",
  "inputs": [
    {
      "name": "contact_id",
      "type": "Integer",
      "is_required": "1",
      "default_value": "",
      "configuration": [],
      "validators": []
    },
    {
      "name": "team_id",
      "type": "Integer",
      "is_required": "1",
      "default_value": "",
      "configuration": [],
      "validators": []
    },
    {
      "name": "teamlid_op_website",
      "type": "OptionGroup",
      "is_required": "0",
      "default_value": "",
      "configuration": {
        "option_group_name": "show_participant_on_website"
      },
      "validators": []
    },
    {
      "name": "donatie_status_teamlid",
      "type": "OptionGroup",
      "is_required": "0",
      "default_value": "",
      "configuration": {
        "option_group_name": "participant_donation_state"
      },
      "validators": []
    }
  ],
  "actions": [
    {
      "name": "huidig_roparun_event",
      "title": "Huidig roparun event",
      "type": "CurrentEvent",
      "configuration": [],
      "mapping": []
    },
    {
      "name": "campage",
      "title": "Campage",
      "type": "GetCampaignFromEvent",
      "configuration": [],
      "mapping": {
        "event_id": "action.huidig_roparun_event.event_id"
      },
      "condition_configuration": ""
    },
    {
      "name": "contact_is_lid_van_team",
      "title": "Contact is lid van team",
      "type": "ContactIsMemberOfTeam",
      "configuration": {
        "throw_error": "1"
      },
      "mapping": {
        "event_id": "action.huidig_roparun_event.event_id",
        "contact_id": "input.contact_id",
        "team_id": "input.team_id"
      }
    },
    {
      "name": "annuleer_teamlid",
      "title": "Annuleer teamlid",
      "type": "UpdateParticipantStatus",
      "configuration": {
        "status": "4"
      },
      "mapping": {
        "event_id": "action.huidig_roparun_event.event_id",
        "contact_id": "input.contact_id"
      }
    },
    {
      "name": "update_donatiestatus_toon_website",
      "title": "Update donatiestatus en toon op website",
      "type": "CreateOrUpdateParticipant",
      "configuration": {
        "status_id": "1",
        "role_id": "6",
        "update_existing": "1"
      },
      "mapping": {
        "event_id": "action.huidig_roparun_event.event_id",
        "contact_id": "input.contact_id",
        "campaign_id": "action.campage.campaign_id",
        "custom_team_member_data_website": "input.teamlid_op_website",
        "custom_team_member_data_donations": "input.donatie_status_teamlid"
      },
      "condition_configuration": ""
    }
  ],
  "default_data_inputs": [],
  "default_data_actions": [
    {
      "name": "donatie_status",
      "title": "donatie_status",
      "type": "SetValue",
      "configuration": {
        "value": "0"
      },
      "mapping": [],
      "condition_configuration": ""
    },
    {
      "name": "teamlid_op_website",
      "title": "teamlid_op_website",
      "type": "SetValue",
      "configuration": {
        "value": "1"
      },
      "mapping": [],
      "condition_configuration": ""
    }
  ]
}